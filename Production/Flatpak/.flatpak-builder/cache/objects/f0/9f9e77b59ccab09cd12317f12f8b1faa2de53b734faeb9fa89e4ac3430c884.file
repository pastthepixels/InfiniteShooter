[Application]
name=com.pastthepixels.InfiniteShooter
runtime=org.freedesktop.Platform/x86_64/20.08
sdk=org.freedesktop.Sdk/x86_64/20.08
base=app/org.godotengine.godot.BaseApp/x86_64/3.3
command=godot-runner

[Context]
shared=ipc;
sockets=x11;pulseaudio;
devices=all;

[Extension com.pastthepixels.InfiniteShooter.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=com.pastthepixels.InfiniteShooter.Debug;